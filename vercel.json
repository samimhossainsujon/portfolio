{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1",
            "headers": {
                "Cache-Control": "s-maxage=3600, stale-while-revalidate"
            }
        }
    ]
}